
<div class="row">
  <div class="col-md-12">


    <div class="card map-popup">
      <img
        src="https://mdbootstrap.com/img/new/standard/nature/184.jpg"
        class="card-img-top"
        alt="..."
        />
      <div class="card-body">
        <h5 class="card-title"><%= project.title %></h5>
        <p class="card-text">
        <h4><%= project.start_date.strftime("%d / %m / %Y") %></h4>
        </p>
        <p>
          <%= link_to "More Info", "/projects/#{project.id}", class: "btn btn-primary map-popup-btn" %>
        </p>
      </div>
    </div>


  </div>
</div>


